{
    "name": "netflex/sdk-template-standard",
    "type": "project",
    "description": "Netflex SDK basic template",
    "keywords": [
        "sdk",
        "netflex",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "fideloper/proxy": "^4.0",
        "laravel/tinker": "^2.0",
        "netflex/sdk": "^2.0",
        "netflex/customers": "^2.0",
        "netflex/pages": "^2.0",
        "netflex/notifications": "^2.0",
        "netflex/scheduler": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/tinkerwell-helper": "^1.1",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
